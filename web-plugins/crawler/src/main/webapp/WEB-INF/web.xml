<?xml version="1.0" encoding="UTF-8" standalone="no"?><!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>

    <display-name>iQser Crawler Servlet</display-name>	

	<servlet>
      <servlet-name>CrawlerServlet</servlet-name>
      <servlet-class>net.sf.iqser.plugin.web.crawler.CrawlerServlet</servlet-class>
    </servlet>

    <servlet-mapping>
      <servlet-name>CrawlerServlet</servlet-name>
      <url-pattern>/crawler</url-pattern>
    </servlet-mapping>

	<welcome-file-list>
    <welcome-file>version.html</welcome-file>
  </welcome-file-list>

</web-app>